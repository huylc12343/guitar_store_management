<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BottomNavBar.StorageNav.StorageDataFragment">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize">

        <TextView
            android:id="@+id/tvStorageData"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="StorageDataFragment"
            android:textSize="@dimen/size30sp"
            android:textStyle="bold"
            android:textAllCaps="true"
            />

        <ImageButton
            android:id="@+id/btnStorageDataToHome"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:src="@drawable/go_back_arrow_svgrepo_com"
            android:padding="15dp"
            android:layout_centerVertical="true"
            android:background="?android:attr/selectableItemBackground"
            />
    </RelativeLayout>
</FrameLayout>